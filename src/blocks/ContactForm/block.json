{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ms-block-demo/contactform",
  "title": "Contact Form",
  "category": "text",
  "description": "Contact Form",
  "keywords": ["contact", "form", "hover"],
  "version": "1",
  "textdomain": "ms-block-demo",
  "editorScript": "file:./index.js",
  "attributes": {
    "postID": {
      "type": "string",
      "default": ""
    },
    "link": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "date": {
      "type": "string",
      "default": ""
    },
    "author": {
      "type": "string",
      "default": ""
    },
    "backgroundColor": {
      "type": "string",
      "default": "'#000000'"
    },
    "borderColor": {
      "type": "string",
      "default": "'#000000'"
    },
    "textColor": {
      "type": "string",
      "default": "#FFFFFF"
    },
    "opacity": {
      "type": "string",
      "default": "1"
    }
  },
  "style": "file:./index.css"
}
